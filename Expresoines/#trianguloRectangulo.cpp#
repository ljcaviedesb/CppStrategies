#include<iostream>
#include<math.h>
int main (){
  char coma=",";
  float cateto1, cateto2, hipotenusa;

  std::cout<<"Escriba el valor del cateto 1 ";
  std::cin>>cateto1>>std::endl;
  std::cout<<"Escriba el valor del cateto 2 ";
  std::cin
  hipotenusa = sqrt(pow(cateto1,2) + pow(cateto2,2));
  std::cout<<"La hipotenusa de este triángulo rectángulo vale: "<<hipotenusa<<std::endl;
  return 0;
}
